<idea-plugin>
  <id>me.catcoder.themeswitcher</id>
  <name>ThemeSwitcher</name>
  <version>0.0</version>
  <vendor email="catcoderr@gmail.com" url="https://vk.com/catcoder">CatCoder</vendor>

  <description><![CDATA[
      Switch theme between Darcula and Default depending by time of day<br>
    ]]></description>

  <idea-version since-build="173.0"/>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <postStartupActivity implementation="me.catcoder.themeswitcher.plugin.ThemeSwitcherApplication"/>
    <applicationService serviceImplementation="me.catcoder.themeswitcher.plugin.settings.ThemeSwitcherSettings"/>
    <applicationConfigurable instance="me.catcoder.themeswitcher.plugin.settings.ThemeSwitcherConfig"/>

  </extensions>


</idea-plugin>